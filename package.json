{
  "name": "gwi-pos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.local -- tsx -r ./preload.js server.ts",
    "dev:next": "next dev --webpack",
    "build": "prisma generate && node scripts/generate-schema-sql.mjs && next build && node scripts/build-server.mjs",
    "schema:sql": "prisma migrate diff --from-empty --to-schema-datamodel prisma/schema.prisma --script > prisma/schema.sql",
    "start": "NODE_ENV=production node -r ./preload.js server.js",
    "start:next": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "setup": "prisma generate && prisma db push && npm run db:seed",
    "db:seed": "dotenv -e .env.local -- tsx prisma/seed.ts",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "fix-orphaned-labels": "npx tsx scripts/fix-orphaned-seat-labels.ts",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:floor-plan": "playwright test floor-plan.spec.ts",
    "test:headed": "playwright test --headed",
    "ws:dev": "SOCKET_PATH=/ws dotenv -e .env.local -- tsx ws-server.ts",
    "ws:build": "node scripts/build-ws-server.mjs",
    "build:all": "npm run build && npm run ws:build",
    "load-test": "npx tsx scripts/load-test.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@csstools/postcss-oklab-function": "^5.0.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@heroicons/react": "^2.2.0",
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/client": "6.19.2",
    "@tailwindcss/postcss": "^4",
    "@types/pdfkit": "^0.17.4",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "dexie": "^4.3.0",
    "framer-motion": "^12.29.2",
    "konva": "^10.2.0",
    "lucide-react": "^0.563.0",
    "next": "16.1.5",
    "pdfkit": "^0.17.2",
    "react": "19.2.3",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "19.2.3",
    "react-konva": "^19.2.1",
    "react-virtuoso": "^4.18.1",
    "reactflow": "^11.11.4",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4",
    "twilio": "^5.12.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^11.0.0",
    "esbuild": "^0.27.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.5",
    "prisma": "6.19.2",
    "tsx": "^4.21.0",
    "typescript": "5.9.3"
  },
  "browserslist": [
    "chrome >= 108",
    "firefox >= 115",
    "safari >= 15.4",
    "edge >= 108"
  ]
}
